<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">

    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <div class="container">
      {.section website}
        <header class="site-header">
          <a href="/">
            <h1 data-content-field="site-title" class="site-title">
              {siteTitle}
            </h1>
          </a>
          <squarespace:navigation navigationId="primary-nav" template="navigation" />
        </header>
      {.end}


      <main data-content-field="main-content" id="main">
        {squarespace.main-content}
      </main>

      <footer>
        <squarespace:block-field id="footer-blocks" columns="12" />
      </footer>

      <script type="text/javascript" src="/scripts/site-bundle.js"></script>

      {squarespace-footers}
    </div>
  </body>
</html>
